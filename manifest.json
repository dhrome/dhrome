{
  "name": "Dhrome",
  "description": "Adds support to the omnibox to quick inteact with Drupal.org.",
  "version": "1.2",
  "manifest_version" : 2,
  "background": {
      "scripts" : ["background.js"]
  },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png"
   },
  "omnibox": {
      "keyword": "dpl"
   },
   "permissions": [ "tabs" ]
}
